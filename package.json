{
  "name": "eslint-plugin-sev-legacy-plugin",
  "version": "0.0.5",
  "description": "Plugin for checking relative paths and public api imports. Checking imports from the lower layer to the upper layer. Supports aliases.For projects with eslint <=9.0.0. .",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "checking relative paths",
    "aliases"
  ],
  "author": "ves-sev",
  "main": "./lib/index.js",
  "exports": "./lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "npm-run-all \"lint:*\"",
    "lint:eslint-docs": "npm-run-all \"update:eslint-docs -- --check\"",
    "lint:js": "eslint .",
    "test": "mocha tests --recursive",
    "update:eslint-docs": "eslint-doc-generator"
  },
  "dependencies": {
    "requireindex": "^1.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "eslint": "^8.0.1",
    "eslint-doc-generator": "^2.0.0",
    "eslint-plugin-eslint-plugin": "^6.0.0",
    "eslint-plugin-n": "^17.0.0",
    "mocha": "^11.0.0",
    "npm-run-all2": "^6.1.2"
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "peerDependencies": {
    "eslint": ">=8.57.0"
  },
  "license": "MIT"
}